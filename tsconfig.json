{
	"include": ["src"],
	"compilerOptions": {
		"outDir": "lib",
		"strict": true,
		"module": "commonjs",
		"target": "es2015",
		"skipLibCheck": true,
		"sourceMap": false,
		"declaration": true,
		"alwaysStrict": true,
		"jsx": "react",
		"moduleResolution": "node",
		"esModuleInterop": true,
    "lib": ["dom", "es2020"]
	}
}
